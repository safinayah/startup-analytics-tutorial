# Google Analytics 4 Configuration
# Copy this file to .env and fill in your actual values

# Your GA4 Property ID (found in GA4 Admin > Property Settings)
# Format: G-XXXXXXXXXX or just the numeric ID
GA4_PROPERTY_ID=G-QDBVBGHBFE

# Path to your Google Cloud service account JSON file
# Download this from Google Cloud Console > IAM & Admin > Service Accounts
GA4_CREDENTIALS_PATH=./credentials/ga4-service-account.json

# Optional: Set to 'true' to enable debug logging
GA4_DEBUG=false

# Optional: Default date range for metrics (in days)
GA4_DEFAULT_DAYS=30
